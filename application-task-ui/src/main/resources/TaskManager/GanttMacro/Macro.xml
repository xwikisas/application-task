    console.log(tasks);
    if(tasks.length == 0) {
      console.log('No tasks to show');
      jQuery("#${divId}").html('No tasks to show');
      return;
    }
    gantt = new Gantt("#${divId}", tasks, {
      view_mode_select: true,
      readonly: true, //doesn't work
      view_mode: 'Day',
      //popup_trigger: 'mouseover',
      //today_button: true, //doesn't work
      //view_mode_select: true, //doesn't work
      show_expected_progress: true,
      view_mode_padding: {
        Day: '3d',
        MONTH: '1m',
      },
      on_click: function (task) {
        console.log('clicked:');
        console.log(task);
        window.location = task.link;
      },
      on_progress_change: function (task, newProgress) {
        if (newProgress == task.editHack.oldProgress) {
          return;
        }
        if (!canEdit(task)) {
          task.progress = task.editHack.oldProgress;
          gantt.bars.forEach(bar =&gt; {
            if (bar.task === task) {
              bar.update_progressbar_position();
              bar.update_handle_position();
            }
          });
          new XWiki.widgets.Notification('$errorOnEdit', 'error', '20');
          return;
        }
        task.editHack.oldProgress = newProgress;
        #set($successEdit = $escapetool.javascript($services.localization.render("taskmanager.gantt.taskEdit.success")))
        //new XWiki.widgets.Notification('$successEdit', 'info', '20');
      },
      on_date_change: function (task, newStartDate, newEndDate) {
        if (!canEdit(task)) {
          gantt.bars.forEach(bar =&gt; {
            if (bar.task === task) {
              bar.update_bar_position({
                x: bar.$bar.ox,
                width: bar.$bar.owidth,
              });
              bar.update_handle_position();
            }
          });
          new XWiki.widgets.Notification('$errorOnEdit', 'error', '20');
          return;
        }
        console.log('date changed:' + newStartDate + ' ' + newEndDate);
        console.log(task);
