var gantt;
function canEdit(task) {
  const userName = "$userName";
  #if ($xcontext.macro.params.readonly == true)
    #set($errorOnEdit = $escapetool.javascript($services.localization.render("taskmanager.gantt.taskEdit.error.readonly")))
  #else
