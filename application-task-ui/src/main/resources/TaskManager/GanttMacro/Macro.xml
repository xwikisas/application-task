      userName == task.assignee ||
      userName == task.reporter ||
      $isSuperAdmin
  ));
}
require(['jquery', "$frappe_js_path"], function(jQuery){
  fetch("${xwiki.getDocument('TaskManager.GanttMacro.JSONTaskSource').getURL('get')}${query}")
